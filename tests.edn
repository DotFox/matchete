#kaocha/v1
{:tests [{:id :unit
          :source-paths ["src"]
          :test-paths ["test"]
          :ns-patterns [".*-test"]}
         {:id :unit-cljs
          :type :kaocha.type/cljs
          :source-paths ["src"]
          :test-paths ["test"]
          :ns-patterns [".*-test"]
          ;; :cljs/timeout 10000                        ; 10 seconds, the default
          ;; :cljs/repl-env cljs.repl.node/repl-env     ; node is the default
          ;; :cljs/repl-env cljs.repl.browser/repl-env
          }]
 ;; :reporter kaocha.report.progress/report
 :plugins [:kaocha.plugin/cloverage :kaocha.plugin/notifier :kaocha.plugin/profiling]}
