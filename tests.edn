#kaocha/v1
{:tests [{:id :unit
          :source-paths ["src"]
          :test-paths ["test"]
          :ns-patterns [".*-test"]}
         {:id :unit-cljs
          :type :kaocha.type/cljs
          :source-paths ["src"]
          :test-paths ["test"]
          :ns-patterns [".*-test"]
          :cljs/timeout 60000
          :cljs/repl-env cljs.repl.node/repl-env}]}
